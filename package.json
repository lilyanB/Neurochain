  "contributors": [
    "David Miller (https://davidmiller.io/)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StartBootstrap/startbootstrap-clean-blog.git"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "5.1.3",
    "debug": "^4.3.2",
    "express": "^4.17.1",
    "mongoose": "^6.0.13"
  },
  "devDependencies": {
    "autoprefixer": "10.3.7",
    "browser-sync": "2.27.5",
    "chokidar": "3.5.2",
    "concurrently": "6.3.0",
    "nodemon": "^2.0.15",
    "postcss": "8.3.9",
    "prettier": "2.4.1",
    "pug": "3.0.2",
    "sass": "1.43.2",
    "shelljs": "0.8.4",
    "upath": "2.0.1"
  },
  "main": "index.js"
